{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "standard-with-typescript", 
        "plugin:import/typescript",
        "plugin:prettier/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "project": "./tsconfig.json"
    },
    "plugins":["@typescript-eslint","prettier" ],
    "rules": {
        "@typescript-eslint/strict-boolean-expressions": 0
    }
}
